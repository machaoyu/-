<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hwua.mapper.SyslogMapper">
    <select id="queryAllSyslog" resultType="Syslog">
        select id,visittime,username,ip,url,executionTime,method from syslog
    </select>
    <insert id="addSyslog">
        insert into syslog values(UUID(),#{visitTime},#{username},#{ip},#{url},#{executionTime},#{method})
    </insert>
</mapper>
